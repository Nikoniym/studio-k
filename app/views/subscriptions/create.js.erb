<% if	 @subscription.persisted? %>
  $(".order").html("<%= escape_javascript(render('order')) %>");
  $('form .answer').text('Заказ оформлен.');
<% else %>
  $('form .answer').html('<%= j render 'shared/short_errors', resource: @subscription%>');
<% end %>

<% if @list_of_subscribers %>
  $(".wrapper_form").html("<%= escape_javascript(render('message')) %>");
<% end %>