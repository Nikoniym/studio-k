$(".timetable.slide_inner .content_wrapper").animate({opacity: "0"}, 500);
setTimeout(function () {
    $(".timetable.slide_inner .content_wrapper").html("<%= escape_javascript(render 'elements/table', time_table: @time_table) %>")
    $(".timetable.slide_inner .content_wrapper table").rowspanizer({ vertical_align: 'middle'});
    $(".timetable.slide_inner .content_wrapper").animate({opacity: "1"}, 500);
},500);
